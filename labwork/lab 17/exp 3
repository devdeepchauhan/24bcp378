# Suppose a date is represented as a tuple (d,m,y).Create two date tuples and find the number of days between the two dates

from datetime import date

date1 = (13, 8, 2006)
date2 = (17, 2, 2025)

def days_between_dates(date1, date2):
   
    d1 = date(date1[2], date1[1], date1[0])  
    d2 = date(date2[2], date2[1], date2[0])
    
    return ((d2 - d1).days)

days_difference = days_between_dates(date1, date2)

print(f"Number of days between {date1} and {date2}: {days_difference} days")
